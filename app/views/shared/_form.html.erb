<%= simple_form_for(plant) do |f| %>
  <%= f.input :title %>
  <%= f.input :price_per_clipping %>
  <%= f.input :description %>
  <%= f.input :number_of_clippings %>
  <%= f.input :color %>
  <%= f.input :care_type %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
